   "source": [
    "# Выбираем случайный файл \n",
    "files = [x for x in imdir.glob('**/*') if x.is_file()]\n",
    "file = str(random.choice(files))\n",
    "img = Image.open(file)\n",
    "plt.imshow(img)\n",
    "plt.xticks([])\n",
    "plt.yticks([])\n",
    "\n",
    "# Инференс модели (генерация подписи)\n",
    "transform = transforms.Compose([\n",
    "    transforms.Resize((356, 356)),\n",
    "    transforms.RandomCrop((299, 299)),\n",
    "    transforms.ToTensor(),\n",
    "    transforms.Normalize((0.5, 0.5, 0.5), (0.5, 0.5, 0.5)),\n",
    "])\n",
    "img_tensor = transform(img).cuda()\n",
    "model_emb_128.eval()\n",
    "sentence = ''\n",
    "for word in model_emb_128.caption_image(\n",
    "    img_tensor.unsqueeze(0), dataset.vocab):\n",
    "    sentence += word + ' '\n",
    "print(sentence)"
   ]
  },
