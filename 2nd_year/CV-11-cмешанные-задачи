    "# Загрузка сохраненных моделей (на случай остановки ядра после обучения)\n",
    "\n",
    "model_emb_128 = CNNtoRNN(128, 128, len(dataset.vocab), 1).cuda()\n",
    "model_emb_128.load_state_dict(torch.load('model_emb_128.pt'))\n",
    "\n",
    "model_emb_256 = CNNtoRNN(256, 128, len(dataset.vocab), 1).cuda()\n",
    "model_emb_256.load_state_dict(torch.load('model_emb_256.pt'))"
   ]
