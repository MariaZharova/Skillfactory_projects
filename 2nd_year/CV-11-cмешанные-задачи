    "# Обучаем две модели с раным размером \"embedding layer\"\n",
    "\n",
    "model_emb_128 = train(\n",
    "    embed_size=128,\n",
    "    hidden_size=128,\n",
    "    vocab_size=len(dataset.vocab),\n",
    "    num_layers=1,\n",
    "    learning_rate=3e-4,\n",
    "    num_epochs=3\n",
    ")\n",
    "torch.save(model_emb_128.state_dict(), 'model_emb_128.pt')\n",
    "\n",
    "model_emb_256 = train(\n",
    "    embed_size=256,\n",
    "    hidden_size=128,\n",
    "    vocab_size=len(dataset.vocab),\n",
    "    num_layers=1,\n",
    "    learning_rate=3e-4,\n",
    "    num_epochs=3\n",
    ")\n",
    "torch.save(model_emb_256.state_dict(), 'model_emb_256.pt')"
   ]
  },
